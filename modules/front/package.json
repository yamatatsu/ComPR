{
  "name": "ghanymede-front",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server -d",
    "build": "webpack -p",
    "predeploy": "yarn build",
    "deploy": "dotenv -e .env.deploy ./scripts/deploy",
    "update-schema": "curl https://developer.github.com/v4/public_schema/schema.public.graphql > ./schema/github.graphql",
    "generate-gql-types": "graphql-codegen --config ./schema/codegen.yml"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "apollo-boost": "^0.4.9",
    "graphql": "^15.1.0",
    "monaco-editor": "^0.20.0",
    "monaco-markdown": "^0.0.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.15.3",
    "@graphql-codegen/typescript": "^1.15.3",
    "@graphql-codegen/typescript-resolvers": "^1.15.3",
    "@types/react": "^16.9.36",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/uuid": "^8.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^6.0.2",
    "css-loader": "^3.5.3",
    "dotenv-cli": "^3.1.0",
    "dotenv-webpack": "^1.8.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "monaco-editor-webpack-plugin": "^1.9.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.5",
    "upbin": "^0.9.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
